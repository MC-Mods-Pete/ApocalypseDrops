{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${version}",
	"name": "${mod_name}",
	"description": "This Mod add Zombie drops that will be quite useful in a Zombie-Apocalypse.",
	"authors": [
		"Pete MC Mods, mcmods.pete@gmail.com"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/zombie-apocalypse-drops",
		"sources": "https://github.com/MC-Mods-Pete/ApocalypseDrops",
		"issues": "https://github.com/MC-Mods-Pete/ApocalypseDrops/issues"
	},
	"license": "MIT",
	"icon": "assets/apocalypsedrops/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "net.petemc.apocalypsedrops.ApocalypseDrops"
		],
		"client": [
		  "net.petemc.apocalypsedrops.ApocalypseDropsClient"
		],
		"fabric-datagen": [
		  "net.petemc.apocalypsedrops.ApocalypseDropsDataGenerator"
		],
		"modmenu": [
		  "net.petemc.apocalypsedrops.util.ModMenuIntegration"
		]
	},
	"mixins": [
		"apocalypsedrops.mixins.json"
	],
	"depends": {
		"fabricloader": "${fabric_version_range}",
		"minecraft": "${mc_version_range}",
		"java": "${java_version_range}",
		"fabric-api": "*",
		"cloth-config": ">=16.0.141"
	},
	"suggests": {
		"modmenu": ">=12.0.0-beta.1",
		"another-mod": "*"
	}
}
